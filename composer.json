    "autoload-dev": {
        "psr-4":{
            "SwoftRewrite\\Task\\Unit\\":"test/unit/",
            "SwoftRewrite\\Task\\Unit\\":"test/unit/",
            "SwoftRewrite\\Task\\Testing\\":"test/testing/"
        }
    }
}
